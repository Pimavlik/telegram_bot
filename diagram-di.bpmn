        <dc:Bounds x="1525" y="30" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_AllItemsScanned_di" bpmnElement="Gateway_AllItemsScanned" isMarkerVisible="true">
        <dc:Bounds x="1700" y="45" width="50" height="50" />
      </bpmndi:BPMNShape>
