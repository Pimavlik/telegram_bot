        <dc:Bounds x="1407" y="192" width="36" height="36" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_PayCashOrOther_di" bpmnElement="Task_PayCashOrOther">
        <dc:Bounds x="2275" y="170" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_ReceiveCashAndGiveChange_di" bpmnElement="Task_ReceiveCashAndGiveChange">
        <dc:Bounds x="2425" y="170" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_Start_To_GoToStore_di" bpmnElement="Flow_Start_To_GoToStore">
        <di:waypoint x="93" y="70" />
        <di:waypoint x="175" y="70" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_GoToStore_To_TakeBasket_di" bpmnElement="Flow_GoToStore_To_TakeBasket">
        <di:waypoint x="275" y="70" />
        <di:waypoint x="325" y="70" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_TakeBasket_To_PickGoods_di" bpmnElement="Flow_TakeBasket_To_PickGoods">
        <di:waypoint x="425" y="70" />
        <di:waypoint x="475" y="70" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_PickGoods_To_GoToCash_di" bpmnElement="Flow_PickGoods_To_GoToCash">
        <di:waypoint x="575" y="70" />
        <di:waypoint x="625" y="70" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_GoToCash_To_PutOnBelt_di" bpmnElement="Flow_GoToCash_To_PutOnBelt">
        <di:waypoint x="725" y="70" />
