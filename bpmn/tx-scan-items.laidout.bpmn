      <bpmn:sequenceFlow id="Yes" name="Да" targetRef="Scan" />
      <bpmn:sequenceFlow id="No" name="Нет" targetRef="TE" />
    </bpmn:transaction>
    <bpmn:endEvent id="E" name="End" />
    <bpmn:sequenceFlow id="f1" sourceRef="S" targetRef="TxScan" />
    <bpmn:sequenceFlow id="f2" sourceRef="TxScan" targetRef="E" />
